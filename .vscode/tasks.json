{
    "tasks": [
        {
            "type": "process",
            "label": "before run make debug",
            "command": "/usr/bin/make",
            "args": [
                "debug"
            ],
            "group": {
                "kind": "build"
            }
        },
        {
            "type": "process",
            "label": "before run make driver",
            "command": "/usr/bin/make",
            "args": [
                "driver"
            ],
            "group": {
                "kind": "build"
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}